fulldata <- read.csv("final_nfl.csv", header=TRUE)
fulldata[mapply(is.infinite, fulldata)] <- NA #recode infinite values as NA
library(ggplot2)
library(grid)
library(gridExtra)
#Pick vs. Combine Results
g1 <- ggplot(fulldata, aes(x=Height, y=Pick)) + geom_point(color="black") + theme(panel.background = element_rect(fill = "#f7f7f7"), axis.title.y = element_text(size = rel(2)), axis.title.x = element_text(size = rel(2)))
g2 <- ggplot(fulldata, aes(x=Weight, y=Pick)) + geom_point(color="black") + theme(axis.title.y = element_blank(), axis.text.y = element_blank(), axis.ticks.y = element_blank(), panel.background = element_rect(fill = "#f7f7f7"), axis.title.x = element_text(size = rel(2)))
g3 <- ggplot(fulldata, aes(x=Yd40, y=Pick)) + geom_point(color="black") + theme(axis.title.y = element_blank(), axis.text.y = element_blank(), axis.ticks.y = element_blank(), panel.background = element_rect(fill = "#f7f7f7"), axis.title.x = element_text(size = rel(2))) + labs(x="40 Yard Dash")
g4 <- ggplot(fulldata, aes(x=Vertical, y=Pick)) + geom_point(color="black") + theme(axis.title.y = element_blank(), axis.text.y = element_blank(), axis.ticks.y = element_blank(), panel.background = element_rect(fill = "#f7f7f7"), axis.title.x = element_text(size = rel(2))) + labs(x="Vertical Jump")
g5 <- ggplot(fulldata, aes(x=Bench, y=Pick)) + geom_point(color="black") + theme(panel.background = element_rect(fill = "#f7f7f7"), axis.title.y = element_text(size = rel(2)), axis.title.x = element_text(size = rel(2))) + labs(x="Bench Press")
g6 <- ggplot(fulldata, aes(x=Broad, y=Pick)) + geom_point(color="black") + theme(axis.title.y = element_blank(), axis.text.y = element_blank(), axis.ticks.y = element_blank()) + theme(axis.title.y = element_blank(), axis.text.y = element_blank(), axis.ticks.y = element_blank(), panel.background = element_rect(fill = "#f7f7f7"), axis.title.x = element_text(size = rel(2))) + labs(x="Broad Jump")
g7 <- ggplot(fulldata, aes(x=Cone3, y=Pick)) + geom_point(color="black") + theme(axis.title.y = element_blank(), axis.text.y = element_blank(), axis.ticks.y = element_blank(), panel.background = element_rect(fill = "#f7f7f7"), axis.title.x = element_text(size = rel(2))) + labs(x="Three Cone Drill")
g8 <- ggplot(fulldata, aes(x=Shuttle, y=Pick)) + geom_point(color="black") + theme(axis.title.y = element_blank(), axis.text.y = element_blank(), axis.ticks.y = element_blank(), panel.background = element_rect(fill = "#f7f7f7"), axis.title.x = element_text(size = rel(2))) + labs(x="Shuttle Run")
grid.arrange(g1, g2, g3, g4, g5, g6, g7, g8, nrow=2)
